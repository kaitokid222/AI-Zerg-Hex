                UnitGroupAddUnitGroup(lv_group, UnitLastCreatedGroup());
                libNtve_gf_CreateUnitsAtPoint2(3, "RangedMinionHell", 0, 7, PointFromName(("Hydra " + IntToString(lv_i))));
