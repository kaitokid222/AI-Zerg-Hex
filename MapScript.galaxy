                libNtve_gf_CreateUnitsAtPoint2(1, "WizardMinion", 0, 7, PointFromName(("Hydra " + IntToString(lv_i))));
                if ((lv_i <= 3)) {
                    UnitIssueOrder(UnitLastCreated(), OrderTargetingPoint(AbilityCommand("move", 0), PointFromId(1352576071)), c_orderQueueReplace);
                    UnitGroupAdd(gv_hell_forces_north, UnitLastCreated());
                }
                else {
                    UnitIssueOrder(UnitLastCreated(), OrderTargetingPoint(AbilityCommand("move", 0), PointFromId(1723757530)), c_orderQueueReplace);
                    UnitGroupAdd(gv_hell_forces_south, UnitLastCreated());
                }
            }

            lv_i += 1;
        }
    }

    return true;
}

