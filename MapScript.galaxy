            lv_fort = UnitGroupRandomUnit(UnitGroup("Hellfort", 7, RegionCircle(PointFromName(("Hydra " + IntToString(lv_i))), 4.5), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 1), c_unitCountAlive);
